package baekjoon;

import java.io.*;
import java.util.*;

public class Main_17135_캐슬디펜스 {

	static ArrayList<int[]> enemy;
	static ArrayList<int[]> origin;
	static int N, M, D, max, kill;
	static int numbers[];

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));
		StringBuilder sb = new StringBuilder();
		StringTokenizer st;

		st = new StringTokenizer(br.readLine(), " ");
		N = Integer.parseInt(st.nextToken());
		M = Integer.parseInt(st.nextToken());
		D = Integer.parseInt(st.nextToken());
		int[][] map = new int[N][M]; // N + 1행에는 성이 있고 궁수를 둘 수 있음
		numbers = new int[3]; // 궁수 위치 조합 만들기 위한 배열
		enemy = new ArrayList<int[]>(); // 적의 위치를 담을 배열
		origin = new ArrayList<int[]>();

		int num;
		for (int i = 0; i < N; i++) {
			st = new StringTokenizer(br.readLine(), " ");
			for (int j = 0; j < M; j++) {
				num = Integer.parseInt(st.nextToken());
				if (num == 1) {
					enemy.add(new int[] { i, j });
					origin.add(new int[] { i, j });
				}
				map[i][j] = num;
			}
		}

//		Collections.sort(enemy, new Comparator<int[]>() {
//			@Override
//			public int compare(int[] o1, int[] o2) {
//				return o2[0] - o1[0];
//			}
//		});
//		Collections.sort(origin, new Comparator<int[]>() {
//			@Override
//			public int compare(int[] o1, int[] o2) {
//				return o2[0] - o1[0];
//			}
//		});
		comb(0, 0);
		System.out.println(max);
		br.close();
		bw.append(sb.toString());
		bw.flush();
		bw.close();
	}

	private static void comb(int start, int cnt) {
		if (cnt == 3) {
			int res = game();
			if (max < res)
				max = res;
			kill = 0;
			for (int[] o : origin) {
				enemy.add(new int[] { o[0], o[1] });
			}
			return;
		}
		

		for (int i = start; i < M; i++) {
			numbers[cnt] = i;
			comb(i + 1, cnt + 1);
		}

	}

	private static int game() {

		while (enemy.size() != 0) {
			Set<Integer> attack = new HashSet<Integer>();
			int distMin, minIdx, removeIdx;
			for (int i = 0; i < 3; i++) {
				distMin = Integer.MAX_VALUE;
				minIdx = Integer.MAX_VALUE;
				removeIdx = -100;
				for (int j = 0; j < enemy.size(); j++) {
					int dist = Math.abs(N - enemy.get(j)[0]) + Math.abs(numbers[i] - enemy.get(j)[1]);
					if (dist <= D) {
						if (dist == distMin) { // 거리가 같을 때는 왼쪽값 사용
							if (enemy.get(j)[1] < minIdx) {
								minIdx = enemy.get(j)[1];
								removeIdx = j;
							}
						} else if (dist < distMin) {
							minIdx = enemy.get(j)[1];
							removeIdx = j;
							distMin = dist;
						}
					}

				}
				if(removeIdx != -100)
					attack.add(removeIdx);
			}
			
			ArrayList<Integer> sortIdx = new ArrayList<>(attack);
			Collections.sort(sortIdx, Collections.reverseOrder());
			attack.clear();
			
			for(int i = 0; i < sortIdx.size(); i++) {
				int num = sortIdx.get(i);
				kill++;
				enemy.remove(num);
			}
			
			int len = enemy.size();
			for (int i = len - 1; i >= 0; i--) {
				enemy.get(i)[0] += 1;
				if (enemy.get(i)[0] >= N) {
					enemy.remove(i);
				}
			}
		}
		return kill;
	}

}
