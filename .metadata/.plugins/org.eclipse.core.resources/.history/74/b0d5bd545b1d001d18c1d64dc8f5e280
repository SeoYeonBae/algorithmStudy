package jungol;

import java.io.*;
import java.util.*;

public class Main_1828_냉장고 {

	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st;
		
		int N = Integer.parseInt(br.readLine());
		int[][] refrigerator = new int[N][N];
		
		for (int i = 0; i < N; i++) {
			st = new StringTokenizer(br.readLine(), " ");
			refrigerator[i][0] = Integer.parseInt(st.nextToken());
			refrigerator[i][1] = Integer.parseInt(st.nextToken());
		}
		
		Arrays.sort(refrigerator, new Comparator<int[]>() {

			@Override
			public int compare(int[] o1, int[] o2) {
				return o1[1] - o2[1];
			}
		});
		
		System.out.println(refrigerator[0][0] + "  " + refrigerator[0][1]);
		System.out.println(refrigerator[1][0] + "  " + refrigerator[1][1]);
		System.out.println(refrigerator[2][0] + "  " + refrigerator[2][1]);
		System.out.println(refrigerator[3][0] + "  " + refrigerator[3][1]);
	}
	
}
